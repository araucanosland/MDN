                                            <label class="control-label">Rut</label>
                                            <input class="form-control" id="afi_rut" type="text" value="17.024.783-1" readonly="readonly">
                                        </div>
                                    </div>
                                    <div class="col-sm-5">
                                        <div class="form-group">
                                            <label class="control-label">Nombres</label>
                                            <input class="form-control" id="afi_nombres" type="text" value="Carlos Antonio Pradenas Pérez" readonly="readonly">
                                        </div>
                                    </div>
                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <label class="control-label segmento_texto">Segmento</label>
                                            <input class="form-control" id="afi_segmento" type="text" value="Privados" readonly="readonly">
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="form-group">
                                            <label class="control-label">Oficina Asignación</label>
                                            <input class="form-control" id="OficinaAsig" type="text" value="OficinaAsig" readonly="readonly">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-sm-3">
