
@{
    ViewBag.Title = "Flujo Captura Tarjeta TAM";
}
<link href="~/Assets/tam/deps/vue-form-wizard.min.css" rel="stylesheet">
<link href="~/Assets/plugins/bootstrap-validator/bootstrapValidator.min.css" rel="stylesheet">
<link rel="stylesheet" href="https://unpkg.com/vue-select@latest/dist/vue-select.css">
<style>
    .msg-part {
        font-weight: bold;
    }

    input.datepicker {
        height: 34px;
    }

    li {
        text-align: left;
    }
</style>
<div id="bdy_busqueda_tam"></div>
<div id="page-title">
    <div class="row">
        <div class="col-sm-8">
            <div class="form-group"></div>
        </div>
        <div class="col-sm-4">
            <div class="form-group">
                <div class="input-group custom-search-form">
                    <input type="text" class="form-control" id="txtRutComercial" placeholder="Buscar Afiliado Credito" maxlength="10">
                    <span class="input-group-btn">
                        <button class="btn btn-default" type="submit" id="bTam3" style="padding-bottom:5px;"><i class="ion-search"></i></button>
                    </span>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="page-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="tab-base">
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a data-toggle="tab" href="#tab-index-tam" aria-expanded="true" id="tab_index_tam" class="tab-principal">Ingreso</a>
                    </li>
                    <li class="">
                        <a data-toggle="tab" href="#tab-busqueda-tam" aria-expanded="false" id="tab_busqueda_tam" class="tab-principal">Búsqueda</a>
