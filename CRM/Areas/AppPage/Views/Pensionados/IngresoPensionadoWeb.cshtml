<!--Masked Input [ OPTIONAL ]-->
<script src="~/Assets/plugins/masked-input/jquery.maskedinput.min.js"></script>
<!--Magic Checkbox [ OPTIONAL ]-->
<link href="~/Assets/plugins/magic-check/css/magic-check.min.css" rel="stylesheet">

<link href="~/Assets/plugins/bootstrap-validator/bootstrapValidator.min.css" rel="stylesheet">
<!--Bootstrap Select [ OPTIONAL ]-->
<link href="~/Assets/plugins/bootstrap-select/bootstrap-select.min.css" rel="stylesheet">
<script src="~/Assets/plugins/bootbox/bootbox.min.js"></script>



<div id="page-content">
    <div id="contRetencionPen">
        <input type="hidden" id="id_lead" name="id_lead" />
        <div class="tab-base">
            <ul id="myTab" class="nav nav-tabs">
                <li class="active">
                    <a data-toggle="tab" href="#tab_ingreso_web" aria-expanded="true" id="tab_ingreso" class="tab-principal">Ingreso</a>
                </li>
                <li class="">
                    <a data-toggle="tab" href="#tab_mis_casos" aria-expanded="true" id="tab_historial" class="tab-principal">Historial</a>
                </li>
                <li class="">
                    <a data-toggle="tab" href="#tab_contactabilidad_web" aria-expanded="true" id="tab_historial" class="tab-contactabilidad">Contactabilidad</a>
                </li>

            </ul>
            <div class="tab-content">
                <div id="tab_ingreso_web" class="tab-pane fade active in">
                    <div class="row">
                        <div class="col-sm-12" id="divFormularioPen">
                            <div class="panel" style="border-radius: 8px;">
                                <div class="panel-heading">
                                    <h3 class="panel-title">MODULO RETENCIÓN DE PENSIONADOS WEB</h3>
                                </div>
                                <div class="form-horizontal">
                                    <div class="panel-body">

                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">SUCURSAL</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control limpiar" id="txtSucursalretencionweb" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">RUT</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control numero limpiar" placeholder="Ingrese RUT sin guión ni dígito verificador. " id="txtRutPensionadoWeb" maxlength="8" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">NOMBRES</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control limpiar" id="txtNombreretencionweb" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">FONO CONTACTO</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control limpiar" id="txtFonoretencionweb" maxlength="15">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">EMAIL</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control limpiar" id="txtEmailretencionweb" maxlength="100">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">DOMICILIO</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control limpiar" id="txtDomicilioWeb" maxlength="60">
                                            </div>
                                        </div>
                                        <div class="form-group" style="display:none">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">FECHA AFILIACIÓN</label>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control limpiar" id="txtFechaAfiliacion" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">FECHA ATENCIÓN</label>
                                            <div class="col-sm-6">
                                                <div id="dp-component-pen-retencion">
                                                    <div class="input-group date">
                                                        <input type="text" class="form-control" id="ges_pen-retencion_web" name="ges_pen-retencion_web">
                                                        <span class="input-group-addon"><i class="demo-pli-calendar-4"></i></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">ESTADO FINAL</label>
                                            <div class="col-sm-6">
                                                <select id="dllEstadoFinalWeb" name="dllEstadoFinalWeb" class="form-control limpiar">
                                                    <option value="">SELECCIONE...</option>
                                                    <option value="Retenido">RETENIDO</option>
                                                    <option value="Desafiliado">DESAFILIADO</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">MOTIVO DESAFILIACIÓN</label>
                                            <div class="col-sm-6">
                                                <select id="dllMotivoDesafWeb" name="dllMotivoDesafWeb" class="form-control" v-model="modelos.motivoAfi" disabled>
                                                    <option value="">SELECCIONE...</option>
                                                    <option v-for="estadoDesaf in filtros.motivoAfi" :value="estadoDesaf.id">{{estadoDesaf.estado}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="demo-is-inputnormal" class="col-sm-3 control-label">MOTIVO RETENCIÓN</label>
                                            <div class="col-sm-6">
                                                <select id="dllMotivoRetencionWeb" name="dllMotivoRetencionWeb" class="form-control" v-model="modelos.motivoRet" disabled>
                                                    <option value="">SELECCIONE...</option>
                                                    <option v-for="estadoRetecion in filtros.motivoRet" :value="estadoRetecion.id">{{estadoRetecion.estado}}</option>
                                                </select>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="demo-textarea-input">COMENTARIOS</label>
                                            <div class="col-md-6">
                                                <textarea id="txtComentariosWeb" rows="9" class="form-control limpiar"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="msjregsitro"></div>
                                    <div class="panel-footer">
                                        <div class="row">
                                            <div class="col-sm-9 col-sm-offset-3">
                                                <button class="btn btn-danger" type="submit" v-on:click="guardaRegistroRetencionWeb">GUARDAR</button>
                                            </div>
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

                <div id="tab_mis_casos" class="tab-pane fade">
                    <div class="panel" style="border-radius: 7px;">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="demo-pli-information icon-fw"></i></h3>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-sm-2 text-xs-center">
                                    <div class="form-group">
                                        <label class="control-label">RUT</label>
                                        <input id="txtRutFiltro" type="text" placeholder="Ej. 12345678" class="form-control numero" maxlength="8" autocomplete="off" disabled>
                                    </div>
                                </div>
                                <div class="col-sm-4 text-xs-center">
                                    <div class="form-group">
                                        <label class="control-label">NOMBRE</label>
                                        <input id="txtNombreFiltro" type="text" class="form-control" maxlength="10" autocomplete="off" disabled>
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-lg-12">
                            <div class="panel" style="border-radius: 7px;">
                                <div class="panel-heading">
                                    <h3 class="panel-title">MIS CASOS</h3>
                                </div>
                                <div class="panel-body">
                                    <table data-toggle="table"
                                           data-sort-name="id"
                                           data-page-list="[]"
                                           data-page-size="50"
                                           data-search="false"
                                           data-pagination="true" id="tblMiscasosWeb" style="font-size: 12px">
                                        <thead>
                                            <tr>
                                                <th data-field="fecha_gestion" data-sortable="true" data-formatter="formatoFecha">FECHA GESTION</th>
                                                <th data-field="estado_final" data-sortable="true">ESTADO</th>
                                                <th data-field="motivo_desafiliacion" data-sortable="true">MOTIVO DESAFILIACION</th>
                                                <th data-field="motivo_retencion" data-sortable="true">MOTIVO RETENCION</th>
                                                <th data-field="comentarios" data-sortable="true">COMENTARIOS</th>



                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>




                <div id="tab_contactabilidad_web" class="tab-pane fade">
                    <div class="row">
                        <button type="button" id="btn-add-contac" class="btn btn-primary btn-xs">+</button>
                    </div>
                    <div class="mar-btm" style="display: none;" id="formulario-contacto">
                        <form id="form-registro-contacto" action="#" method="post">

                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <div class="btn-group bootstrap-select">
                                            <label class="control-label">Tipo de Contacto</label>
                                            <select class="selectpicker" id="cbtippContac" name="cbtippContac">
                                                <option value="">Seleccione..</option>
                                                <option value="1">Celular</option>
                                                <option value="2">Teléfono</option>
                                                <option value="3">Mail</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <div class="btn-group bootstrap-select">
                                            <label class="control-label">Clasificación de Contacto</label>
                                            <select class="selectpicker" id="cbClasificacionConctac" name="cbClasificacionConctac">
                                                <option value="">Seleccione..</option>
                                                <option value="1">Personal</option>
                                                <option value="2">Comercial</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label class="control-label">Datos de Contacto</label>
                                        <input class="form-control" id="afi_NewContacto" type="text" value="" maxlength="150" name="afi_NewContacto">
                                    </div>
                                </div>
                            </div>
                            <button class="btn btn-primary" id="btGuardaContacto" type="submit">Guardar Contacto</button>
                        </form>
                    </div>
                    <div class="row">
                        <table class="table table-hover table-vcenter" style="font-size: 11px;">
                            <thead>
                                <tr>
                                    <th>Gestión</th>
                                    <th>Datos Contacto</th>
                                    <th>Tipo contacto</th>
                                    <th>Indice</th>
                                    <th>Ultimo Contacto Valido</th>
                                </tr>
                            </thead>
                            <tbody id="bdy_datos_contactos"></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="panel-footer">
      
        <div class="row">
            <div class="col-sm-9 col-sm-offset-3">
                <button class="btn btn-warning" id="bt-bolver">Volver</button>
            </div>
        </div>
    </div>
</div>

@section script{
    <script src="~/Assets/js/vue.js"></script>
    <script src="~/Assets/plugins/fooTable/dist/footable.all.min.js"></script>
    <script src="~/Assets/plugins/bootstrap-validator/bootstrapValidator.min.js"></script>
    <script src="~/Assets/plugins/fooTable/dist/footable.all.min.js"></script>
    <script src="~/Assets/plugins/bootstrap-validator/bootstrapValidator.min.js"></script>
    <script src="~/Assets/plugins/bootstrap-select/bootstrap-select.min.js"></script>
    <script src="~/Assets/plugins/chosen/chosen.jquery.min.js"></script>
    <script src="~/Assets/plugins/bootstrap-table/bootstrap-table.min.js"></script>
    <script src="~/Assets/plugins/bootstrap-table/locale/bootstrap-table-es-ES.min.js"></script>
    <script src="~/Assets/plugins/chosen/chosen.jquery.min.js"></script>
    <script src="~/Assets/plugins/switchery/switchery.min.js"></script>
    <script src="~/Assets/plugins/bootstrap-select/bootstrap-select.min.js"></script>
    <script src="~/Assets/tam/deps/sweetalert.js"></script>
    <script src="~/Assets/js.App/src/retencion-pensionados.js?g=@Guid.NewGuid().ToString()"></script>

}
