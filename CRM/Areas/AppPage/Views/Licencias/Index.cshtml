                        }

                        if (e.IngresoData.FormatoLM == "Manual" && e.IngresoData.viaIngresoLicenica == 1 && e.IngresoData.DescripcionEstadoRevision == 'Conforme' && e.IngresoData.EtapaActual == 'Conforme') {
                            $wrapper = $wrapper + (`<a class="btn btn-primary mar-lft" title="Editar" href="/motor/App/Licencias/auditorialicencia?ci=${e.IngresoData.CodIngreso}&tab=secundaria"><i class="ion-search"></i></a>`);

                        }

                        if (e.IngresoData.FormatoLM == "Manual" && e.IngresoData.viaIngresoLicenica == 1 && e.IngresoData.DescripcionEstadoRevision == 'Conforme' && e.IngresoData.EstadoLicencia == 'Reparada') {
                            $wrapper = $wrapper + (`<a class="btn btn-primary mar-lft" title="Editar" href="/motor/App/Licencias/auditorialicencia?ci=${e.IngresoData.CodIngreso}&tab=secundaria"><i class="ion-search"></i></a>`);

