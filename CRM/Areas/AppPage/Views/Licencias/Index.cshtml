                buttons: {
                    confirm: {
                        label: 'Si, eliminar',
                        className: 'btn-warning'
                    },
                    cancel: {
                        label: 'No, no estoy seguro/a',
                        className: 'btn-danger'
                    }
                },
                callback: function (result) {
                    if (result) {

                        $.SecGetJSON(BASE_URL + "/motor/api/Licencias/eliminar-licencia-ingresada", { CodIngreso: codIngreso }, function (response) {

                            if (response.Estado == "OK") {
                                $.niftyNoty({
                                    type: 'success',
                                    icon: 'pli-like-2 icon-2x',
                                    message: 'LM Eliminada exitosamente',
                                    container: 'floating',
                                    timer: 5000
                                });
                            }
                        })
                        cargador.CargaDatosTablaIndex($("#ddlresponsableindex").val(), 'Todos', $("#txt_folio_LM_index").val(), $("#flt_dia_index_desde").val(), $("#flt_dia_index_hasta").val(), $("#ddlformatoLMindex").val());
                        Cargador.CargaDatosEncabezado($("#fechahoy").val());

                        cargador.CargaDatosTablaTab3Auditoria($("#txt_folio_LM").val(), $("#flt_dia_auditoria_desde").val(), $("#flt_dia_auditoria_hasta").val(), "-2", getCookie("oficina"), $("#ddlestadoAuditoria").val(), $("#ddlresponsable").val());
                        cargador.CargaDatosTablaTab3AuditoriaOficinaConteo();
                    }
                }
            });
        }
