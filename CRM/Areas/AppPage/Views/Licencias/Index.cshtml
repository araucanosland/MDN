                        //    $wrapper = $wrapper + (`<a class="btn btn-primary mar-lft" title="Editar" href="/motor/App/Licencias/auditorialicencia?ci=${e.IngresoData.CodIngreso}&tab=secundaria"><i class="ion-edit"></i></a>`);

                        //}


                        $wrapper = $wrapper + (`</td>`);

                        $("#bdy_datos").append($wrapper);
                        total++;

                    });
                    $("#cantLMindex").html("Cantidad de Registros " + total);

                });
                $("#cantLMindex").show();
            },


