                        $wrapper = $wrapper + (`<td>`);
                        if (e.IngresoData.DescripcionEstadoRevision != 'No Econtrada' && e.IngresoData.DescripcionEstadoRevision != 'No Conforme' && e.IngresoData.DescripcionEstadoRevision != 'Conforme') {
                            $wrapper = $wrapper + (`<a class="btn btn-danger float-right" title="Eliminar" href="javascript:eliminar(${e.IngresoData.CodIngreso})"><i class="ion-trash-a"></i></a>`);

