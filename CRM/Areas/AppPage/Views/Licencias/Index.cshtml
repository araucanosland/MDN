                        //if (e.IngresoData.DescripcionEstadoRevision == 'No Conforme' && e.IngresoData.EtapaActual == 'Conforme') {
                        //    $wrapper = $wrapper + (`<a class="btn btn-danger float-right" title="Eliminar" href="javascript:eliminar(${e.IngresoData.CodIngreso})"><i class="ion-trash-a"></i></a>`);

                        //    $wrapper = $wrapper + (`<a class="btn btn-primary mar-lft" title="Editar" href="/motor/App/Licencias/Ingreso?ci=${e.IngresoData.CodIngreso}"><i class="ion-edit"></i></a>`);

                        //}
                        //if (e.IngresoData.DescripcionEstadoRevision == 'No Conforme' && e.IngresoData.EtapaActual == 'Reparada con documentación pendiente') {
                        //    $wrapper = $wrapper + (`<a class="btn btn-danger float-right" title="Eliminar" href="javascript:eliminar(${e.IngresoData.CodIngreso})"><i class="ion-trash-a"></i></a>`);

                        //    $wrapper = $wrapper + (`<a class="btn btn-primary mar-lft" title="Editar" href="/motor/App/Licencias/Ingreso?ci=${e.IngresoData.CodIngreso}"><i class="ion-edit"></i></a>`);

                        //}


                        //if (e.FormatoLM == "Manual" && e.viaIngresoLicenica == 1 && e.descripcionEstadoRevision == 'Pendiente' && e.EstadoLicencia == 'Devuelta') {
                        //    $wrapper = $wrapper + (`<a class="btn btn-primary mar-lft" title="Editar" href="/motor/App/Licencias/Ingreso?ci=${e.CodIngreso}"><i class="ion-edit"></i></a>`);

                        //}


                        //if (e.viaIngresoLicenica != 1) {
                        //    $wrapper = $wrapper + (`<a class="btn btn-primary mar-lft" title="Editar" href="/motor/App/Licencias/Ingreso?ci=${e.CodIngreso}"><i class="ion-edit"></i></a>`);

                        //}


                        //$wrapper = $wrapper + (`<a class="btn btn-warning float-right" title="Seguimiento" href="/motor/App/Licencias/LineaTiempoLM?ci=${e.IngresoData.CodIngreso}")"><i class="ion-map"></i></a>`);

