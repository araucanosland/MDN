    <script>
        var cargador = {

            cargaLC: function (CodIng) {

                $.SecGetJSON(BASE_URL + "/motor/api/Licencias/licencia-data-auditoria", { codIngreso: CodIng }, function (response) {
                    console.log(response);

                    $("#AfiliadoRut").val(response.RutAfiliado);
                    $("#AfiliadoRut").mask('99.999.999-*')
                    $("#AfiliadoRut").trigger("blur");
                    if (response.FormatoLM == 'Mixta') {
                        $("#radio-formato-mixto-lm").prop('checked', true)
                        $("#radio-formato-mixto-lm").trigger("change")

                        //if ($("#FolioLic").val().length == 9) {
                        //    $('#FolioLic').mask('9-9999999-*');
                        //}
                        //else {
                        //    $('#FolioLic').mask('9-99999999-*');
                        //}
                    }


                    //---valida si existe compin
                    if (response.compincentralizado == 'Si') {
                        $('#divcompin').show();
                        $('#compin').val(response.compincentralizado);
                        $('#subcomision').val(response.Subcomision);
                        $('#subido_a_plataforma_compin').val(response.Subido_a_plataforma_compin.toFecha());
                    }
                    else {
                        $('#divcompin').hide();

                    }


                    if (response.FormatoLM == "Manual") {
                        $('#radio-formato-mixto-lm').prop("checked", false);
                        $('#radio-formato-manual-lm').prop("checked", true);
                    }
                    else {
                        $('#radio-formato-mixto-lm').prop("checked", true);
                        $('#radio-formato-manual-lm').prop("checked", false);
                    }

                    $("#FolioLic").val(response.FolioLicencia);
                    $("#enviado").val(response.descripcionquienEnvia);
