        <script>

            var cargador = {
                cargaGestion1: function (codIngreso, tipoG) {
                    
                    $.SecGetJSON(BASE_URL + "/motor/api/Licencias/lista-gestion-oficina", { codingreso: codIngreso, tipog: tipoG }, function (response) {

                        //$("#number_one").html(menus.Lm_Verde);
